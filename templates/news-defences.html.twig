{% set defences = page.header.defences.items %}


<section id="defences" class="columns">
	<div class="column col-4">
		<h3>{{ page.header.title }}</h3>
	</div>
	<div class="column col-8">
		{% for defence_year in defences %}
			<section class="defence-year-item">
				<h4 class="defence-year-label">{{defence_year.label}}</h4>
				{% for defence in defence_year.items %}
					<div class="news-list-item">
						{{ defence.label }}
					</div>
				{% endfor %}
			</section>
		{% endfor %}
	</div>
</section>
